<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services
    http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <defaults autowire="true" autoconfigure="true" />

        <service public="true" id="SymfonyProbeBundle\Controller\HealthController" class="SymfonyProbeBundle\Controller\HealthController">
            <tag name="container.service_subscriber" />
        </service>
        <service public="true" id="SymfonyProbeBundle\Controller\PingController" class="SymfonyProbeBundle\Controller\PingController">
            <tag name="container.service_subscriber" />
        </service>
    </services>
</container>
